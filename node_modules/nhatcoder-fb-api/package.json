{
  "name": "nhatcoder-fb-api",
  "version": "2.6.5",
  "description": "Facebook chat API that doesn't rely on XMPP",
  "scripts": {
    "test": "mocha",
    "lint": "eslint **.js",
    "prettier": "prettier utils.js src/* --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhatcoder2003/nhatcoder-fb-api.git"
  },
  "keywords": [
    "facebook",
    "chat",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/nhatcoder2003/nhatcoder-fb-api/issues"
  },
  "author": "Nhatcoder",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^2.11.0",
    "cheerio": "^1.0.0-rc.12",
    "https-proxy-agent": "^4.0.0",
    "mqtt": "^4.3.7",
    "npmlog": "^1.2.0",
    "request": "^2.53.0",
    "websocket-stream": "^5.5.0"
  },
  "engines": {
    "node": ">=4.x"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "mocha": "^7.0.1",
    "prettier": "^1.11.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "es2017": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        1,
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ]
    }
  },
  "homepage": "https://github.com/nhatcoder2003/nhatcoder-fb-api#readme",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "types": "nhatcoder.d.ts"
}
